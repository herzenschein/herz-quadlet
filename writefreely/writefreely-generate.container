[Container]
Image=docker.io/writeas/writefreely:latest
ContainerName=writefreely-generate
AutoUpdate=registry
#Notify=true
LogDriver=journald

Volume=/path/to/config.ini:/go/config.ini:Z

Network=writefreely.network

User=daemon
Group=daemon
UserNS=keep-id

Exec=config generate

[Service]
Restart=no
